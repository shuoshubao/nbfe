{
    "name": "@nbfe/tools",
    "version": "0.2.17",
    "description": "工具库",
    "author": "shuoshubao <759979885@qq.com>",
    "repository": {
        "url": "https://github.com/shuoshubao/nbfe",
        "type": "git"
    },
    "license": "ISC",
    "files": [
        "dist"
    ],
    "main": "dist/index.js",
    "scripts": {
        "test": "npx ava --verbose",
        "test:watch": "npx ava --verbose --watch",
        "coverage": "npx nyc ava",
        "docs:generate": "nodemon bin --watch ./bin",
        "docs:dev": "node bin && rm -rf _site/ && ydoc serve",
        "docs:build": "node bin && rm -rf _site/ && ydoc build",
        "coverage:summary": "npx nyc --reporter=lcov --reporter=text-summary ava",
        "build": "rm -rf dist && npx rollup -c",
        "prepublishOnly": "npm run build"
    },
    "dependencies": {
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@ava/babel": "^1.0.1",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/preset-env": "^7.11.0",
        "@nbfe/js2html": "^1.1.7",
        "@rollup/plugin-babel": "^5.1.0",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-replace": "^5.0.1",
        "@types/babel__core": "^7.1.9",
        "async-validator": "^3.4.0",
        "ava": "^3.12.1",
        "browser-env": "^3.3.0",
        "core-js": "^3.16.0",
        "dox": "^1.0.0",
        "esm": "^3.2.25",
        "highlight.js": "^11.7.0",
        "moment": "^2.27.0",
        "node-fetch": "^2.6.0",
        "nyc": "^15.1.0",
        "pako": "^2.1.0",
        "rollup": "^2.67.2",
        "rollup-plugin-terser": "^7.0.2"
    }
}
